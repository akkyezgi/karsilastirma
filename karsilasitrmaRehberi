        function showSelected() {
            var selectedBoats = [];
            var checkedBoats = document.querySelectorAll('input[name^="boat"]:checked');

            checkedBoats.forEach(function (boat) {
                selectedBoats.push(boat.value);
            });

            var boatInfo = document.getElementById('boatInfo');
            boatInfo.innerHTML = "";

            selectedBoats.forEach(function (selected) {
                var boatDiv = document.createElement("div");
                boatDiv.innerHTML = "<h2>" + selected + " Özellikleri</h2>";

                for (var variant in boats[selected]) {
                    boatDiv.innerHTML += "<h3>" + variant + ":</h3>" +
                        "<strong>Uzunluk:</strong> " + boats[selected][variant]["Uzunluk"] + "<br>" +
                        "<strong>Draft:</strong> " + boats[selected][variant]["Draft"] + "<br>" +
                        "<strong>Genişlik:</strong> " + boats[selected][variant]["Genişlik"] + "<br>" +
                        "<strong>Yolcu Kapasitesi:</strong> " + boats[selected][variant]["Yolcu Kapasitesi"] + "<br>" +
                        "<strong>Azami Hız:</strong> " + boats[selected][variant]["Azami Hız"] + "<br>" +
                        "<strong>Jet-ski`siz Ağırlığı:</strong> " + boats[selected][variant]["Jet-ski`siz Ağırlığı"] + "<br>" +         
                        "<strong>Kategori:</strong> " + boats[selected][variant]["Kategori"] + "<br>" +
                        "<strong>Tente:</strong> " + boats[selected][variant]["Tente"] + "<br>" +
                        "<strong>Wakeboard Kulesi:</strong> " + boats[selected][variant]["Wakeboard Kulesi"] + "<br>" +   
                        "<strong>Sealver Çıkartma:</strong> " + boats[selected][variant]["Sealver Çıkartma"] + "<br>" +
                        "<strong>Depolama Alanı:</strong> " + boats[selected][variant]["Depolama Alanı"] + "<br>" +
                        "<strong>Yolcu ile Ağırlığı:</strong> " + boats[selected][variant]["Yolcu ile Ağırlığı"] + "<br>" +
                        "<strong>Raf:</strong> " + boats[selected][variant]["Raf"] + "<br>" +
                        "<strong>Elektrik Sistemi:</strong> " + boats[selected][variant]["Elektrik Sistemi"] + "<br>" +
                        "<strong>Ses Sistemi Birleşim (Gold-Silver):</strong> " + boats[selected][variant]["Ses Sistemi Birleşim (Gold-Silver)"] + "<br>" +
                        "<strong>Minderli Masa:</strong> " + boats[selected][variant]["Minderli Masa"] + "<br>" +
                        "<strong>Navigasyon Işıkları:</strong> " + boats[selected][variant]["Navigasyon Işıkları"] + "<br>" +
                        "<strong>Sealver Çıkartma:</strong> " + boats[selected][variant]["Sealver Çıkartma"] + "<br>" +

            
                        


                        // Diğer özellikler
                        "<br>";
                }

                var boatImageLink = document.createElement("a"); // Link elementi oluşturuldu
                boatImageLink.href = resimler[selected]["yonlendirmeLinki"]; // Resmin yönlendirme linki atanıyor

                var boatImage = document.createElement("img");
                boatImage.src = resimler[selected]["link"];
                boatImage.alt = selected + " resmi";
                boatImageLink.appendChild(boatImage); // Resim link içerisine ekleniyor

                boatDiv.appendChild(boatImageLink); // Resim linki div içerisine ekleniyor

                boatInfo.appendChild(boatDiv);
            });
        }
    </script>
